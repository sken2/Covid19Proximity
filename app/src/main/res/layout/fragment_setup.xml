<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HistoryFragment">

    <Button
        android:id="@+id/button_to_control"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="132dp"
        android:text="Control"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_to_contact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text="Contact"
        app:layout_constraintBottom_toBottomOf="@+id/button_to_control"
        app:layout_constraintStart_toEndOf="@+id/button_to_control" />

    <Button
        android:id="@+id/button_to_aboutthis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text="help"
        app:layout_constraintBottom_toBottomOf="@+id/button_to_control"
        app:layout_constraintStart_toEndOf="@+id/button_to_contact" />
</androidx.constraintlayout.widget.ConstraintLayout>

